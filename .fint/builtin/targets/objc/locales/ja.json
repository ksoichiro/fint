{
  "rulesets": [
    {
      "id": "ObjectiveCSource",
      "modules": [
        {
          "id": "pattern_match",
          "rules": [
            {"id": "WhitespaceAfterElse", "message": "elseの後にはスペースを入れてください"},
            {"id": "WhitespaceBeforeElse", "message": "elseの前にはスペースを入れてください"},
            {"id": "WhitespaceAfterIf", "message": "ifの後にはスペースを入れてください"},
            {"id": "WhitespaceAfterFor", "message": "forの後にはスペースを入れてください"},
            {"id": "WhitespaceAfterSwitch", "message": "switchの後にはスペースを入れてください"},
            {"id": "WhitespaceAfterEqualOperator", "message": "'='の後にはスペースを入れてください"},
            {"id": "WhitespaceBeforeEqualOperator", "message": "'='の前にはスペースを入れてください"},
            {"id": "WhitespaceAfterAndOperator", "message": "'&'の後にはスペースを入れてください"},
            {"id": "WhitespaceBeforeAndOperator", "message": "'&'の前にはスペースを入れてください"},
            {"id": "WhitespaceAfterOrOperator", "message": "'|'の後にはスペースを入れてください"},
            {"id": "WhitespaceBeforeOrOperator", "message": "'|'の前にはスペースを入れてください"},
            {"id": "NoWhitespaceAfterOpenParenthesis", "message": "'('の後にはスペースを入れないでください"},
            {"id": "NoWhitespaceBeforeCloseParenthesis", "message": "')'の前にはスペースを入れないでください"},
            {"id": "NoWhitespaceBeforeCloseBracket", "message": "']'の前にはスペースを入れないでください"},
            {"id": "NoWhitespaceBeforeSemicolon", "message": "セミコロンの前にはスペースを入れないでください"},
            {"id": "WhitespaceBetweenParAndBrace", "message": "')'と'{'の間にはスペースを入れてください"},
            {"id": "WhitespaceAfterComma", "message": "','の後にはスペースを入れてください"},
            {"id": "WhitespaceAtStartOfComment", "message": "'//'とコメントの間にはスペースを入れてください"},
            {"id": "WhitespaceBetweenBracketAndMessage", "message": "']'とメッセージの間にはスペースを入れてください"},
            {"id": "TrailingWhitespace", "message": "行末のスペースを削除してください"}
          ]
        },
        {
          "id": "indent",
          "rules": [
            {"id": "Whitespaces", "message": "インデントにはタブではなくスペースを使用してください"}
          ]
        },
        {
          "id": "max_length",
          "rules": [
            {"id": "ExceedMaxLength", "message": "1行の長さが%d文字を超えています"}
          ]
        }
      ]
    }
  ]
}
